{
  "name": "social-auth-server",
  "version": "0.0.1",
  "description": "Server for React/Node Social auth",
  "main": "server.js",
  "scripts": {
    "travis": "npm t -- --runInBand --maxWorkers=4",
    "test": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles",
    "dev": "npm run pg-up && npm run pg-seed && NODE_PATH=./ nodemon ./server",
    "start": "npm run pg-up && npm run pg-seed && NODE_PATH=./ node ./bin/",
    "down": "npm run pg-down && npm run redis-down",
    "up": "npm run pg-up && npm run pg-seed",
    "redis-down": "redis-cli flushall",
    "pg-down": "knex migrate:rollback --knexfile ./db/config.js",
    "pg-make": "knex migrate:make $NAME --knexfile ./db/config.js",
    "pg-seed": "knex seed:run --knexfile ./db/config.js",
    "pg-up": "knex migrate:latest --knexfile ./db/config.js"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.15.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-boom": "^2.0.0",
    "express-session": "^1.15.6",
    "jest": "^24.1.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "passport-reddit": "^0.2.4",
    "passport-twitter": "^1.0.4",
    "pg": "^7.8.0",
    "pg-native": "^3.0.0",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "supertest": "^3.4.2",
    "uuid": "^3.3.2"
  },
  "author": "maikelmclauflin",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.10",
    "passport-mock": "0.0.3"
  }
}
