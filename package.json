{
  "name": "social-auth-server",
  "version": "0.0.1",
  "description": "Server for React/Node Social auth",
  "main": "server.js",
  "scripts": {
    "lint": "npm run lint:js",
    "lint:js": "npm run lint:eslint:fix -- server",
    "security": "npm run lint && npm audit",
    "docker-build": "docker-compose build",
    "docker-up": "docker-compose up $npm_package_config_dockerservices",
    "docker-migrate-up": "docker-compose -f docker-compose.yml run --rm -w /usr/src/app kuhts-web npm run pg-up && npm run pg-seed",
    "docker-migrate-down": "docker-compose -f docker-compose.yml run --rm -w /usr/src/app kuhts-web npm run pg-down",
    "create-db": "node ./bin/create.js",
    "travis": "$npm_package_scripts_test --maxWorkers=4",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "dev": "npm run pg-up && npm run pg-seed && NODE_PATH=./ nodemon ./server",
    "start": "npm run pg-up && npm run pg-seed && NODE_PATH=./ node ./bin/",
    "down": "npm run pg-down && npm run redis-down",
    "up": "npm run pg-up && npm run pg-seed",
    "redis-down": "redis-cli flushall",
    "pg-down": "knex migrate:rollback --knexfile ./db/config.js",
    "pg-make": "knex migrate:make $NAME --knexfile ./db/config.js",
    "pg-seed": "knex seed:run --knexfile ./db/config.js",
    "pg-up": "knex migrate:latest --knexfile ./db/config.js",
    "lint:eslint": "eslint --ignore-path !./.eslintrc.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path !./.eslintrc.js --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "config": {
    "dockerservices": "kuhts-postgres kuhts-redis"
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.15.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-boom": "^2.0.0",
    "express-session": "^1.15.6",
    "jest": "^24.1.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "passport-reddit": "^0.2.4",
    "passport-twitter": "^1.0.4",
    "pg": "^7.8.0",
    "pg-native": "^3.0.0",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "supertest": "^3.4.2",
    "uuid": "^3.3.2"
  },
  "author": "maikelmclauflin",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "nodemon": "^1.18.10",
    "passport-mock": "0.0.3",
    "prettier": "^1.16.4",
    "validator": "^10.11.0"
  }
}
